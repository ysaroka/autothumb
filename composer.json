    "license": "ISC",
    "authors": [
        {
            "name": "Yauhen Saroka",
            "email": "yauhen.saroka@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "imanee/imanee": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
